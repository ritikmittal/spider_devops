  git commit -m "1st commit from Develop branch"
  git checkout master
  touch commit2_master.txt
  open commit2_master.txt
  git add .
  git commit -m "2nd commit to master branch"
  git log
  git merge Develop
  git branch
  git branch spider
  git checkout spider
  touch content.txt
  open co
  open content.txt
  git add .
  git commit -m "content added"
  touch commit2_spider.txt
  open commit2_spider.txt
  mv content.txt commit1_spider.txt
  git add .
  git status
  git commit -m "2nd commit to spider branch"
  git checkout master
  git merge spider
  git branch -d spider
  git branch -d Develop
